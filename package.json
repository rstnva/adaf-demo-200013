{
  "name": "adaf-dashboard",
  "private": true,
  "version": "1.0.0",
  "description": "ADAF Dashboard - Sistema Integrado de Agentes de Inteligencia Financiera",
  "scripts": {
    "dev": "next dev --port 3005",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
  "test": "vitest",
  "test:coverage": "vitest --coverage",
  "test:wsp": "vitest -c vitest.wsp.config.ts run",
  "test:wsp:coverage": "vitest -c vitest.wsp.config.ts run --coverage",
    "test:ui": "vitest --ui",
    "test:ingestion": "vitest tests/news.ingestion.test.ts tests/tvl.ingestion.test.ts",
    "test:agents": "vitest tests/agent.worker.test.ts",
    "test:e2e": "vitest tests/system.e2e.test.ts",
    "clean": "rm -rf .next",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "worker:start": "node -r esbuild-register src/lib/agents/worker.ts",
    "redis:start": "redis-server",
    "redis:cli": "redis-cli",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.9",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/bcryptjs": "^3.0.0",
    "@types/react-beautiful-dnd": "^13.1.8",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "ioredis": "^5.4.1",
    "lucide-react": "^0.544.0",
    "next": "15.5.4",
    "node-cron": "^3.0.3",
    "playwright": "^1.55.1",
    "prom-client": "^15.1.3",
    "react": "19.1.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "19.1.1",
    "recharts": "^2.15.4",
    "rss-parser": "^3.13.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.2",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.5.2",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "esbuild": "^0.23.1",
    "esbuild-register": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.5.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jsdom": "^27.0.0",
    "lint-staged": "^14.0.1",
    "msw": "^2.6.6",
    "postcss": "^8.4.47",
    "prettier": "^3.5.1",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.1",
    "typescript": "^5.9.2",
    "vaul": "^1.1.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
