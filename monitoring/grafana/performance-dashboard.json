{
  "dashboard": {
    "id": null,
    "title": "ADAF Dashboard - Performance Overview",
    "description": "Comprehensive performance monitoring for ADAF Dashboard application",
    "tags": ["adaf", "performance", "monitoring"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "environment",
          "type": "custom",
          "options": [
            {"text": "Production", "value": "prod"},
            {"text": "Staging", "value": "staging"},
            {"text": "Development", "value": "dev"}
          ],
          "current": {"text": "Production", "value": "prod"}
        },
        {
          "name": "instance", 
          "type": "query",
          "query": "label_values(up{service=\"adaf-dashboard\"}, instance)",
          "refresh": 1,
          "multi": true,
          "includeAll": true
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "System Health Overview",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "API Response Time (p95)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 1},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(adaf_api_request_duration_seconds_bucket{environment=\"$environment\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "p95 Response Time",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.2},
                {"color": "red", "value": 0.5}
              ]
            }
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto"
        }
      },
      {
        "id": 3,
        "title": "Cache Hit Rate",
        "type": "stat", 
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 1},
        "targets": [
          {
            "expr": "avg(adaf_cache_hit_ratio{environment=\"$environment\", instance=~\"$instance\"}) * 100",
            "legendFormat": "Cache Hit Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 1,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 85}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Database Query Time (p95)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 1},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(adaf_db_query_duration_seconds_bucket{environment=\"$environment\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "p95 DB Query Time",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 4,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.05},
                {"color": "red", "value": 0.1}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Request Rate (RPS)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 1},
        "targets": [
          {
            "expr": "sum(rate(adaf_api_requests_total{environment=\"$environment\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "Requests/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "decimals": 1
          }
        }
      },
      {
        "id": 6,
        "title": "API Performance Trends",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 5}
      },
      {
        "id": 7,
        "title": "API Response Time Distribution",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(adaf_api_request_duration_seconds_bucket{environment=\"$environment\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(adaf_api_request_duration_seconds_bucket{environment=\"$environment\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "p95", 
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(adaf_api_request_duration_seconds_bucket{environment=\"$environment\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time",
            "unit": "s",
            "min": 0
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "avg": true,
          "current": true,
          "max": true
        }
      },
      {
        "id": 8,
        "title": "Request Rate by Endpoint",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "targets": [
          {
            "expr": "sum by (endpoint) (rate(adaf_api_requests_total{environment=\"$environment\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "unit": "reqps",
            "min": 0
          }
        ]
      },
      {
        "id": 9,
        "title": "Database Performance",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 14}
      },
      {
        "id": 10,
        "title": "Database Query Performance",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 15},
        "targets": [
          {
            "expr": "sum(rate(adaf_db_query_duration_seconds_bucket{environment=\"$environment\", instance=~\"$instance\"}[5m])) by (le)",
            "format": "heatmap",
            "refId": "A"
          }
        ],
        "heatmap": {
          "xBucketSize": null,
          "xBucketNumber": null,
          "yBucketSize": null,
          "yBucketNumber": null
        },
        "yAxis": {
          "unit": "s",
          "decimals": 3
        }
      },
      {
        "id": 11,
        "title": "Database Connections",
        "type": "graph", 
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 15},
        "targets": [
          {
            "expr": "adaf_db_connections_active{environment=\"$environment\", instance=~\"$instance\"}",
            "legendFormat": "Active Connections - {{pool}}",
            "refId": "A"
          },
          {
            "expr": "adaf_db_connections_max{environment=\"$environment\", instance=~\"$instance\"}",
            "legendFormat": "Max Connections - {{pool}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Connections",
            "min": 0
          }
        ],
        "fill": 1,
        "linewidth": 2
      },
      {
        "id": 12,
        "title": "Cache Performance",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 23}
      },
      {
        "id": 13,
        "title": "Cache Hit Rates by Type",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "adaf_cache_hit_ratio{environment=\"$environment\", instance=~\"$instance\"} * 100",
            "legendFormat": "{{cache_type}} Hit Rate",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Hit Rate %",
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        ],
        "thresholds": [
          {
            "value": 85,
            "colorMode": "critical",
            "op": "lt",
            "fill": true,
            "line": true,
            "yAxis": "left"
          }
        ]
      },
      {
        "id": 14,
        "title": "Cache Operations Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "sum by (operation, result) (rate(adaf_cache_operations_total{environment=\"$environment\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "{{operation}} ({{result}})",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Operations/sec",
            "unit": "ops",
            "min": 0
          }
        ]
      },
      {
        "id": 15,
        "title": "Business Metrics",
        "type": "row", 
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 32}
      },
      {
        "id": 16,
        "title": "Strategy Calculation Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 33},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(adaf_strategy_calculation_duration_seconds_bucket{environment=\"$environment\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "p95 Calculation Time",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(adaf_strategy_calculation_duration_seconds_bucket{environment=\"$environment\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "p50 Calculation Time",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Calculation Time",
            "unit": "s",
            "min": 0
          }
        ]
      },
      {
        "id": 17,
        "title": "Market Data Lag",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 33},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(adaf_market_data_lag_seconds_bucket{environment=\"$environment\", instance=~\"$instance\"}[5m]))",
            "legendFormat": "p95 Data Lag - {{symbol}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Lag Time",
            "unit": "s",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 60,
            "colorMode": "critical",
            "op": "gt",
            "fill": false,
            "line": true,
            "yAxis": "left"
          }
        ]
      },
      {
        "id": 18,
        "title": "Error Analysis",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 41}
      },
      {
        "id": 19,
        "title": "Error Rate by Endpoint",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 42},
        "targets": [
          {
            "expr": "sum by (endpoint) (rate(adaf_api_requests_total{environment=\"$environment\", instance=~\"$instance\", status_code=~\"4..|5..\"}[5m])) / sum by (endpoint) (rate(adaf_api_requests_total{environment=\"$environment\", instance=~\"$instance\"}[5m])) * 100",
            "legendFormat": "{{endpoint}} Error Rate",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Error Rate %",
            "unit": "percent",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 1,
            "colorMode": "critical",
            "op": "gt",
            "fill": true,
            "line": true,
            "yAxis": "left"
          }
        ]
      },
      {
        "id": 20,
        "title": "Top 5 Slowest Endpoints",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 42},
        "targets": [
          {
            "expr": "topk(5, histogram_quantile(0.95, rate(adaf_api_request_duration_seconds_bucket{environment=\"$environment\", instance=~\"$instance\"}[5m])))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true
              },
              "renameByName": {
                "endpoint": "Endpoint",
                "Value": "Response Time (s)"
              }
            }
          }
        ]
      },
      {
        "id": 21,
        "title": "Resource Utilization",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 50}
      },
      {
        "id": 22,
        "title": "CPU and Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 51},
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{environment=\"$environment\", instance=~\"$instance\"}[5m]) * 100",
            "legendFormat": "CPU Usage % - {{instance}}",
            "refId": "A"
          },
          {
            "expr": "process_resident_memory_bytes{environment=\"$environment\", instance=~\"$instance\"} / 1024 / 1024",
            "legendFormat": "Memory Usage MB - {{instance}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "CPU % / Memory MB",
            "min": 0
          }
        ]
      },
      {
        "id": 23,
        "title": "Garbage Collection Performance",
        "type": "graph", 
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 51},
        "targets": [
          {
            "expr": "rate(nodejs_gc_duration_seconds_total{environment=\"$environment\", instance=~\"$instance\"}[5m])",
            "legendFormat": "GC Duration - {{gc_type}}",
            "refId": "A"
          },
          {
            "expr": "rate(nodejs_gc_runs_total{environment=\"$environment\", instance=~\"$instance\"}[5m])",
            "legendFormat": "GC Runs - {{gc_type}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Duration (s) / Runs per sec",
            "min": 0
          }
        ]
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(adaf_deployment_timestamp[1d])",
          "iconColor": "rgba(0, 211, 255, 1)",
          "titleFormat": "Deployment",
          "textFormat": "Version: {{version}}"
        },
        {
          "name": "Alerts",
          "datasource": "prometheus", 
          "enable": true,
          "expr": "ALERTS{service=\"adaf-dashboard\"}",
          "iconColor": "rgba(255, 96, 96, 1)",
          "titleFormat": "Alert: {{alertname}}",
          "textFormat": "{{description}}"
        }
      ]
    },
    "links": [
      {
        "title": "ADAF Runbooks",
        "url": "http://runbooks.adaf.local",
        "icon": "doc",
        "type": "link"
      },
      {
        "title": "Performance Testing",
        "url": "http://performance.adaf.local",
        "icon": "bolt", 
        "type": "link"
      }
    ]
  }
}